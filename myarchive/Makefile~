NAME	=	my_archive

SRC	=	my_archive.c

OBJ	=	$(SRC:.c=.o)

CFLAGS	+=	-I ./include

LFLAGS	+=	-L./lib/ -lmy -L./lib/my/ -lprintf

all:	$(NAME)

$(NAME):$(OBJ)
	gcc -o $(NAME) $(OBJ) $(LFLAGS) -lncurses


clean:
	rm -f $(OBJ)

fclean: clean
	rm -f $(NAME)

re:	fclean all

.PHONY: re clean fclean all
